		\[  \sum^{\infty}_{n=1}   \frac{3}{(3n+1)(3n+4)}  \]
		
		\item[(ii)]
		\[  \sum^{\infty}_{n=1}   \frac{4}{(2n+1)(2n+3)}  \]
		
		\item[(iii)]
		\[  \sum^{\infty}_{n=1}   \frac{5}{(5n+1)(5n+6)}  \]
		
		\item[(iv)]
		\[  \sum^{\infty}_{n=1}  \frac{6}{(6n+1)(6n+7)}  \]
		.
	\end{itemize}
\end{multicols}

