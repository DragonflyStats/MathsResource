				\begin{multicols}{2}
					\begin{enumerate}
						
						\item		 \[ f(x) = 2x^2 + 4\,\] %%%={{noprint|ˆtf'(x)=4x</math>}}}}
						\item		 \[ f(x) = 3\sqrt[3]{x}\, \]
						\item		 \[ f(x) = 2x^5+8x^2+x-78\,\] %%%={{noprint|ˆtf'(x)=10x^4+16x+1</math>}}}}
						\item		 \[ f(x) = 7x^7+8x^5+x^3+x^2-x\,\] %%%={{noprint|ˆtf'(x)=49x^6+40x^4+3x^2+2x-1</math>}}}}
						\item		\[ f(x) = \frac{1}{x^2}+3x^\frac{1}{3}\,\] %%%={{noprint|ˆtf'(x)=\frac{-2}{x^3}+\frac{1}{\sqrt[3]{x^2}}</math>}}}}
						
						\item		 \[ f(x) = 3x^{15} + \frac{1}{17}x^2 +\frac{2}{\sqrt{x}} \,\] %%%={{noprint|ˆtf'(x)=45x^{14}+\frac{2}{17}x-\frac{1}{x\sqrt{x}}</math>}}}}
						\item		 \[ f(x) = \frac{3}{x^4} - \sqrt[4]{x} + x \,\] %%%={{noprint|ˆtf'(x)=\frac{-12}{x^5}-\frac{1}{4\sqrt[4]{x^3}}+1</math>}}}}
						\item		 \[ f(x) = 6x^{1/3}-x^{0.4} +\frac{9}{x^2} \,\] %%%={{noprint|ˆtf'(x)=\frac{2}{\sqrt[3]{x^2}}-\frac{0.4}{x^{0.6}}-\frac{18}{x^3}</math>}}}}
						\item		 \[ f(x) = \frac{1}{\sqrt[3]{x}} + \sqrt{x} \,\] %%%={{noprint|ˆtf'(x)=\frac{-1}{3x\sqrt[3]{x}}+\frac{1}{2\sqrt{x}}</math>}}}}
					\end{enumerate}
				\end{multicols}
				\newpage
